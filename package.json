{
  "version": "0.0.0",
  "name": "nicoespeon.github.io",
  "description": "Nicolas Carlo personal blog",
  "private": true,
  "repository": "git+https://github.com/nicoespeon/nicoespeon.github.io.git",
  "keywords": ["nicoespeon", "blog"],
  "author": "Nicolas Carlo <nicolascarlo.espeon@gmail.com>",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "serve": "gatsby develop",
    "lint": "eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "clean": "npm run clean:public",
    "clean:public": "rm -rf public",
    "build": "gatsby build",
    "github-files":
      "touch public/.nojekyll && echo 'www.nicoespeon.com' > public/CNAME",
    "predeploy": "npm run lint",
    "deploy":
      "gatsby build --prefix-paths && npm run github-files && gh-pages -d public -b master"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "gatsby": "2.0.22",
    "gatsby-plugin-catch-links": "2.0.4",
    "gatsby-plugin-favicon": "3.1.5",
    "gatsby-plugin-feed": "2.0.8",
    "gatsby-plugin-google-analytics": "2.0.6",
    "gatsby-plugin-google-fonts": "0.0.4",
    "gatsby-plugin-offline": "2.0.6",
    "gatsby-plugin-react-helmet": "3.0.0",
    "gatsby-plugin-sass": "2.0.1",
    "gatsby-plugin-sharp": "2.0.6",
    "gatsby-plugin-sitemap": "2.0.1",
    "gatsby-remark-autolink-headers": "2.0.12",
    "gatsby-remark-copy-linked-files": "2.0.5",
    "gatsby-remark-images": "2.0.4",
    "gatsby-remark-prismjs": "3.0.2",
    "gatsby-remark-responsive-iframe": "2.0.5",
    "gatsby-remark-smartypants": "2.0.5",
    "gatsby-source-filesystem": "2.0.3",
    "gatsby-transformer-remark": "2.1.7",
    "gatsby-transformer-sharp": "2.1.3",
    "lodash": "4.17.4",
    "node-sass": "4.9.3",
    "prismjs": "1.15.0",
    "react": "16.5.2",
    "react-disqus-comments": "1.2.0",
    "react-dom": "16.5.2",
    "react-helmet": "5.2.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "4.14.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "gh-pages": "1.1.0",
    "husky": "1.3.0",
    "lost": "8.2.0",
    "postcss-pxtorem": "4.0.1",
    "prettier": "1.10.2",
    "pretty-quick": "1.8.0"
  }
}
